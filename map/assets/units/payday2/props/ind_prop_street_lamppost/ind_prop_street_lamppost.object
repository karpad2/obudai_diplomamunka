<dynamic_object>
    <diesel orientation_object="rp_ind_prop_street_lamppost" materials="units/payday2/props/ind_prop_street_lamppost/ind_prop_street_lamppost" />
	<sequence_manager file="units/payday2/props/ind_prop_street_lamppost/ind_prop_street_lamppost" />
    <animations>
        <animation_group name="anim_lamppost" enabled="false">
            <object name="anim" />
        </animation_group>
    </animations>	
    <bodies>
        <body enabled="true" template="static" name="static_body" keyframed="true">
            <object collision_type="mesh_mopp" name="c_lamppost"/>
        </body>        
		<body enabled="true" template="static" name="light_bulb" keyframed="true">
            <object collision_type="mesh_mopp" name="c_light"/>
        </body>
    </bodies>
	
    <decal_surfaces>
        <decal_mesh enabled="true" name="dm_metal" material="sheet_metal" />
        <decal_mesh enabled="true" name="dm_glass" material="glass_breakable" />
    </decal_surfaces>
	
    <effects>
       <effect_spawner name="street_light_flare_blueish" enabled="false" object="e_flare" effect="effects/payday2/particles/flares/street_light_flare_blueish"/>
    </effects>
	
    <graphics>
        <object enabled="true" name="g_lamppost" />
        <object enabled="false" name="g_il" />
        <object enabled="true" shadow_caster="true" name="s_lampost" />
    </graphics>
	
    <lights>
        <light enabled="false" name="lo_omni_01" multiplier="1" far_range="1000" falloff="7" type="omni|specular" />
    </lights>
	
</dynamic_object>
