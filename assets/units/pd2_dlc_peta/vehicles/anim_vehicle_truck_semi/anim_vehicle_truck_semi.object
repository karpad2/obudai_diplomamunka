		<decal_mesh	name="dm_tire_rear_left2_rubber"		enabled="true" material="rubber" />
		<decal_mesh	name="dm_tire_rear_left_rubber"			enabled="true" material="rubber" />
		<decal_mesh	name="dm_tire_rear_right2_rubber"		enabled="true" material="rubber" />
		<decal_mesh	name="dm_tire_rear_right_rubber"		enabled="true" material="rubber" />
		<decal_mesh	name="dm_tire_trailer_left2_rubber"		enabled="true" material="rubber" />
		<decal_mesh	name="dm_tire_trailer_left_rubber"		enabled="true" material="rubber" />
		<decal_mesh	name="dm_tire_trailer_right2_rubber"	enabled="true" material="rubber" />
		<decal_mesh	name="dm_tire_trailer_right_rubber"		enabled="true" material="rubber" />
