		<decal_mesh name="c_printer_door" 	enabled="true" material="plastic"/>
	</decal_surfaces>


	<graphics>
		<object name="g_printer_base" 	enabled="true" />
		<object name="g_drill" 			enabled="false"	/>
		<object name="g_printer_lid" 	enabled="true" />
		<object name="g_printer_door" 	enabled="true" />
		<object name="a_can_01" 		enabled="false"/>
		<object name="a_can_02" 		enabled="false"/>
