<dynamic_object>
    <diesel orientation_object="rp_str_prop_street_crashbarrier" materials="units/payday2/props/str_prop_street_crashbarrier/str_prop_street_crashbarrier" />
 	<sequence_manager file="units/payday2/props/str_prop_street_crashbarrier/str_prop_street_crashbarrier" />
	
	<animations>
	
        <animation_group name="anim_lid" >
            <object enabled="true" name="a_lid" />
        </animation_group>
		
    </animations>
	
    <bodies>
	
        <body enabled="true" template="animated" name="lid" >
            <object collision_type="box" name="c_box_1" padding="-2.5" />
        </body>
		
        <body enabled="true" template="static" name="base" >
            <object collision_type="box" name="c_box_2" padding="-2.5"  />
        </body>
		
    </bodies>
	
    <decal_surfaces>
        <decal_mesh enabled="true" name="dm_lid" material="sheet_metal" />
        <decal_mesh enabled="true" name="dm_base" material="sheet_metal" />
    </decal_surfaces>
	
    <graphics>
        <object enabled="true" name="g_lid" shadow_caster="true" />
        <object enabled="true" name="g_base" />
    </graphics>
	
</dynamic_object>
