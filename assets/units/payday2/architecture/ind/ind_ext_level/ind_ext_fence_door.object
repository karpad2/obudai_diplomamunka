	<animations>
		<animation_group name="anim">
			<object name="anim_door" />
		</animation_group>
	</animations>
	<bodies>
		<body name="static_body" enabled="true" template="static">
			<object name="rp_ind_ext_fence_door" />
			<object name="c_box_nonanim_01" collision_type="box" />
			<object name="c_box_nonanim_02" collision_type="box" />
			<object name="c_box_nonanim_03" collision_type="box" />
		</body>
		<body name="body_door" enabled="true" template="animated">
			<object name="anim_door" />
			<object name="c_box_anim_01" collision_type="box" />
			<object name="c_box_anim_02" collision_type="box" />
			<object name="c_box_anim_03" collision_type="box" />
			<object name="c_box_anim_04" collision_type="box" />
			<object name="c_box_anim_05" collision_type="box" />
		</body>
		<body name="body_door_expl" enabled="true" template="animated">
			<object name="anim_door" />
			<object name="c_box_anim_01" collision_type="box" />
			<object name="c_box_anim_02" collision_type="box" />
			<object name="c_box_anim_04" collision_type="box" />
		</body>
		<body name="mover_blocker" enabled="true" template="mover_blocker" keyframed="true">
			<object name="anim_door" />
			<object name="c_box_player" collision_type="box" />
		</body>
		<body name="body_hitbox_door_handle_out" enabled="true" template="static">
			<object name="rp_ind_ext_fence_door" />
			<object name="c_hitbox_door_handle_out" collision_type="box" padding="-2.5" />
		</body>
	</bodies>
	<decal_surfaces default_material="steel">
	</decal_surfaces>
	<graphics>
		<object name="g_g" 				enabled="true" shadow_caster="false" />
		<object name="g_g_1" 			enabled="true" shadow_caster="false" />
		<object name="g_g_1_expl" 		enabled="false" shadow_caster="true" />
		<object name="g_g_cloth" 		enabled="false" shadow_caster="false" />
		<object name="g_alpha_shadow" 	enabled="true" shadow_caster="true" />
		<object name="s_s" 				enabled="true" shadow_caster="true"  />
		<object name="s_s_1" 			enabled="true" shadow_caster="true"  />
		<graphic_group name="sawicongroup" 					enabled="false" culling_object="g_icon_saw">
			<lod_object name="saw_icon">
					<object name="g_icon" 		enabled="true"  max_distance="450"  max_draw_lod="0" />
					<object name="g_icon_lod1" 	enabled="true"  lod="1" />
			</lod_object>
		</graphic_group>
	</graphics>
</dynamic_object>